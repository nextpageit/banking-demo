@model BankingApplication.Models.Transactions
@if (Model == null)
{
    return;
}
<div id="setting-details" class="row">
  
    @if (Model.TransactionList != null && Model.TransactionList.Any())
    {
        <h1>User Transactions</h1>
        <table class="table table-striped">
            <tr>
                <th>Type</th>
                <th>Amount</th>

            </tr>
            @foreach (var trans in Model.TransactionList)
            {
                <tr>
                    <td>@trans.Type</td>
                    <td>@trans.Amount</td>

                </tr>
            }

        </table>
    }
</div>
